<%= render 'shared/title', title:"Log In" %>

<div class="row">
	<div class="well col-md-6 col-md-offset-3">
		<!--NOT a model-back form!-->
		<%= form_tag '/login' do %>
		<%= label_tag :email %>
		<%= text_field_tag :email %>
		
		<%= label_tag :password %>
		<%= password_field_tag :password %>

		<%= submit_tag 'Login', class:"btn btn-md btn-success"%>
		<% end %>
	</div>
</div>